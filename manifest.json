{
    "name": "MyEpitech Notification",
    "description": "Extension for MyEpitech Notification",
    "version": "1.0.0",
    "permissions": [
        "storage",
        "tabs",
        "cookies"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "src/background.js"
    },
    "action": {
        "default_popup": "src/popup/popup.html",
        "default_title": "MyEpitech Notification"
    },
    "icons": {
        "16": "assets/icons/16.png",
        "32": "assets/icons/32.png",
        "48": "assets/icons/48.png",
        "128": "assets/icons/128.png"
    },
    "manifest_version": 3
}
